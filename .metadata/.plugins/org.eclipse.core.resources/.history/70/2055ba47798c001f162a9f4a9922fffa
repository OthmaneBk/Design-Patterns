package com.esisa.dp.creationa.builder;

import java.util.List;

public class Client {
	private String source="resources/design-patterns.txt";
	
	public Client() {
		application1();
	}
	
	void application1() {
		/*
		 * Construction d'une liste de design patterns recherch√©s par intention
		 * */
		Director directore=new Director(new ConcreteBuilder1());
		List<DesingPattern> result= (List<DesingPattern>)directore.construct(source, "interface");
		
		for (DesingPattern pattern : result) {
			System.out.println(pattern);
		}
	}
	public static void main(String[] args) {
		
	}
}
